<h1 id="welcomePage">Welcome! Please log in</h1>
<form id="signin" name="login" method="POST" action="/users/login">
    <div>
        <label for="name">Username: </label>
        <br>
        <input type="text" name="username" required>
    </div>
    <div>
        <label for="name">Password: </label>
        <br>
        <input type="password" name="password" required>

    </div>
    <div>
        <button id="welcomeSubmit" type="submit">Submit</button>
    </div>
</form>